<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="GetterSetter.GS_Movie" table="movie">
  <id name="id">  
 <generator class="increment"></generator>  
 </id>  
 <property name="movid"></property>  
 <property name="name"></property>  
 <property name="year"></property>  
 <property name="genre"></property>
 <property name="cast"></property>  
 <property name="producer"></property>  
 <property name="director"></property>
 <property name="plot"></property>
   </class>
</hibernate-mapping>
